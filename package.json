{
    "name": "natour",
    "version": "1.0.0",
    "description": "learning node, express and mongoDB",
    "browserslist": "> 0.5%, last 2 versions, not dead",
    "targets": {
        "main": false,
        "default": {
            "includeNodeModules": false
        }
    },
    "main": "app.js",
    "scripts": {
        "start": "nodemon server.js",
        "start:prod": "NODE_ENV=production nodemon server.js",
        "debug": "ndb srver.js",
        "watch:js": "parcel watch ./public/js/index.js --dist-dir ./public/js/bundle --public-url js",
        "build:js": "parcel build ./public/js/index.js --dist-dir ./public/js/bundle --public-url js"
    },
    "author": "yashGupta",
    "license": "ISC",
    "dependencies": {
        "axios": "^1.2.2",
        "bcryptjs": "^2.4.3",
        "compression": "^1.7.4",
        "cookie": "^0.5.0",
        "cookie-parser": "^1.4.6",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "express-mongo-sanitize": "^2.2.0",
        "express-rate-limit": "^6.7.0",
        "helmet": "^6.0.1",
        "hpp": "^0.2.3",
        "html-to-text": "^9.0.3",
        "jsonwebtoken": "^9.0.0",
        "mongoose": "^5.13.15",
        "morgan": "^1.10.0",
        "multer": "^1.4.5-lts.1",
        "nodemailer": "^6.8.0",
        "parser": "^0.1.4",
        "pug": "^3.0.2",
        "sharp": "^0.31.3",
        "slugify": "^1.6.5",
